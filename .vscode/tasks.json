            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": "$msCompile"
        },
        {
            "label": "build (ReleaseStatic)",
            "type": "shell",
            "command": "C:/Program Files/Microsoft Visual Studio/2022/Community/MSBuild/Current/Bin/MSBuild.exe",
            "args": [
                "${workspaceFolder}/NVEnc.sln",
                "/p:Configuration=ReleaseStatic",
                "/p:Platform=x64",
                "/m"
            ],
            "group": {
                "kind": "build",
                "isDefault": false
            },
            "problemMatcher": "$msCompile"
        }
    ]
}
